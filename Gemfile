  gem "jekyll-commonmark"
  gem "jekyll-commonmark-ghpages"
  gem "jekyll-default-layout"
  gem "jekyll-feed"
  gem "jekyll-gist"
  gem "jekyll-paginate"
  gem "jekyll-theme-minimal"
  gem "minima"
  gem "jekyll-sitemap"
  gem "jekyll-minifier"
end